{
  "name": "tsjs-xpx-chain-sdk",
  "version": "0.3.0",
  "description": "Proximax Blockchain sdk for typescript and javascript",
  "scripts": {
    "pretest": "npm run build",
    "test": "mocha --ui bdd --recursive ./dist/test --timeout 90000",
    "test:e2e": "npm run build && mocha --ui bdd --recursive ./dist/e2e --timeout 90000",
    "test:all": "mocha --ui bdd --recursive ./dist/ --timeout 90000",
    "build": "rm -rf dist/ && tsc",
    "test:cov": "nyc --reporter=lcov --reporter=text-lcov npm t && nyc report --reporter=text-lcov",
    "test:coveralls": "npm run test:cov | coveralls"
  },
  "contributors": [
    "Thomas Tran <tldinteractive@gmail.com>",
    "Yaaccount <tnuoccaay@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/proximax-storage/tsjs-xpx-chain-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proximax-storage/tsjs-xpx-chain-sdk.git"
  },
  "publishConfig": {
    "registry": "https://nexus.internal.proximax.io/repository/npm-private/"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/proximax-storage/tsjs-xpx-chain-sdk#readme",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/crypto-js": "^3.1.43",
    "@types/lodash": "^4.14.85",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.0",
    "@types/request": "^2.47.0",
    "@types/request-promise-native": "^1.0.14",
    "@types/utf8": "^2.1.6",
    "@types/ws": "^3.2.0",
    "assert": "^1.4.1",
    "chai": "^4.1.2",
    "config": "^3.1.0",
    "coveralls": "^3.0.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "secure-random": "^1.1.1",
    "ts-mockito": "^2.2.7",
    "ts-node": "^8.2.0",
    "tslint": "^5.8.0",
    "typescript": "^3.4.4",
    "typescript-require": "^0.2.9-1"
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "crypto-js": "^3.1.9-1",
    "flatbuffers": "^1.11.0",
    "js-joda": "^1.6.2",
    "js-sha256": "^0.9.0",
    "js-sha3": "^0.8.0",
    "ripemd160": "^2.0.2",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "utf8": "^2.1.2",
    "ws": "^5.2.0"
  },
  "peerDependencies": {},
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "xpx",
    "proximax",
    "sdk"
  ],
  "author": "Proximax"
}
